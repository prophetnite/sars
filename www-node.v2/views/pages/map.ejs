<!doctype html>
<!-- views/pages/index.ejs -->

<html lang="en">
<head>
	<% include ../partials/head %>
</head>
<body>

	<header>
        <% include ../partials/header %>
        <% include ../partials/sidebar %>
    </header>







        <!-- begin MAIN PAGE CONTENT -->
        <div id="page-wrapper">

            <div class="page-content">

                <!-- begin PAGE TITLE AREA -->
                <!-- Use this section for each page's title and breadcrumb layout. In this example a date range picker is included within the breadcrumb. -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="page-title">
                            <h1><%= pagetitle %>
                                <small>Content Overview</small>
                            </h1>
                            <ol class="breadcrumb">
                                <li class="active"><i class="fa fa-dashboard"></i> <%= pagetitle %></li>
                                <li class="pull-right">
                                    <div id="reportrange" class="btn btn-green btn-square date-picker">
                                        <i class="fa fa-calendar"></i>
                                        <span class="date-range"></span> <i class="fa fa-caret-down"></i>
                                    </div>
                                </li>
                            </ol>
                        </div>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <!-- end PAGE TITLE AREA -->











            </div>
            <!-- /.page-content -->
						<label-default>Map of a device</label-default>
						<div id="map"></div>
						<style>#map {width:450px; height:600px;}</style>
        </div>
        <!-- /#page-wrapper -->
        <!-- end MAIN PAGE CONTENT -->









    <footer>
        <% include ../partials/footer %>
    </footer>

	<!-- fill in  -->
	<script >
		$(document).ready(function() {
			//App.init();
			//MapGoogle.init();
			$("#mapGoogle").googleMap({zoom: 10, // Initial zoom level (optional)
			coords: [48.895651, 2.290569], // Map center (optional)
			type: "HYBRID" // Map type (optional)});
})
	});
	</script>

</body>
</html>
